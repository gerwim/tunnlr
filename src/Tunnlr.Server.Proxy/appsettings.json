{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://0.0.0.0:5107",
        "Protocols": "Http2"
      },
      "Web": {
        "Url": "http://0.0.0.0:5108",
        "Protocols": "Http1"
      }
    }
  },
  "Auth0": {
    "Audience": "https://tunnlr.dev",
    "Domain": "tunnlr.eu.auth0.com",
    "Issuer": "https://tunnlr.eu.auth0.com/"
  },
  "Tunnlr": {
    "Server": {
      "Proxy": {
        "ServedFromWildcard": "https://*.tunnel.tunnlr.dev"
      }
    }
  }
}
